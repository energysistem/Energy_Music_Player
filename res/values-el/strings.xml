    <string name="nowplaying">Παίζεται τώρα</string>

    <!-- Used to indicate the number of albums for an artist -->
    <plurals name="Nalbums">

        <!-- Number of albums is one -->
        <item quantity="one">1 άλμπουμ</item>
        <!-- Number of albums is more than one -->
        <item quantity="other"><xliff:g id="count">%d</xliff:g> άλμπουμ</item>
    </plurals>

    <!-- Used to indicate the number of songs for an album -->

    <plurals name="Nsongs">

        <!-- Number of songs is one -->
        <item quantity="one">1 κομμάτι</item>
        <item quantity="other"><xliff:g id="count">%d</xliff:g> κομμάτια</item>
    </plurals>

    <!-- Toasts after adding song(s) to playlists -->
    <plurals name="NNNtrackstoplaylist">
        <!-- message shown when one song was added -->
        <item quantity="one">1 κομμάτι προστέθηκε στη λίστα αναπαραγωγής</item>
        <!-- message shown when zero or more than one song was added -->
        <item quantity="other"><xliff:g id="number" example="27">%d</xliff:g> κομμάτια προστέθηκαν στη λίστα αναπαραγωγής</item>
    </plurals>

